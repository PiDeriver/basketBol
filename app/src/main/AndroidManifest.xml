<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pideriver.a2017tatorscouttracingapp">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ScoutSignIn">
            android:screenOrientation="landscape"
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MatchSetup"
            android:screenOrientation="landscape" />
        <activity
            android:name=".Match"
            android:screenOrientation="landscape"
            android:theme="@style/generalnotitle"></activity>
        <activity android:name=".Comments" />
        <activity android:name=".CalibrationScreen"
            android:screenOrientation="landscape"></activity>
    </application>

</manifest>